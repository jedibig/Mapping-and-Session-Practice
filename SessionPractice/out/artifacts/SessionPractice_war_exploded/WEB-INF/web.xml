<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>URLRSForm</servlet-name>
        <jsp-file>/URLRSForm.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>URLRSForm</servlet-name>
        <url-pattern>/url-rewrite/form</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CSForm</servlet-name>
        <jsp-file>/CSForm.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>CSForm</servlet-name>
        <url-pattern>/cookie/form</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HFSForm</servlet-name>
        <jsp-file>/HFSForm.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>HFSForm</servlet-name>
        <url-pattern>/hidden-fields/form</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SOForm</servlet-name>
        <jsp-file>/SOForm.jsp</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>SOForm</servlet-name>
        <url-pattern>/session-object/form</url-pattern>
    </servlet-mapping>
</web-app>